{
  "scripts": {
    "scrape-icons": "ts-node src/scrape-pixelarticons.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "prebuild": "rm -rf dist",
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean --treeshake --sourcemap",
    "postbuild": "chmod +x dist/cli.js",
    "dev": "tsc -w",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "name": "@c14mx/pix31",
  "version": "1.0.3",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cli.js",
  "module": "dist/cli.mjs",
  "types": "dist/cli.d.ts",
  "author": "c14mx",
  "license": "MIT",
  "description": "A CLI to add pixelarticons to your React and React Native project.",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.0",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prompts": "^2.4.2",
    "puppeteer": "^24.2.1",
    "svgson": "^5.3.1",
    "term-img": "^3.0.0",
    "terminal-link": "^3.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.13.4",
    "@types/open": "^6.1.0",
    "@types/prompts": "^2.4.9",
    "changelog": "^1.4.2",
    "jest": "^29.0.0",
    "prettier": "^3.5.1",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3"
  },
  "bin": {
    "@c14mx/pix31": "./dist/cli.js"
  },
  "files": [
    "dist",
    "pixelarticons"
  ],
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/c14mx/pix31"
  },
  "bugs": {
    "url": "https://github.com/c14mx/pix31/issues"
  },
  "homepage": "https://github.com/c14mx/pix31#readme"
}
